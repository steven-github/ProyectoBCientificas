
@{
    ViewBag.Title = "RamasCientificasEditar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>RamasCientificasEditar</h2>

<form class="form-horizontal">
    <div class="form-group">
        <label for="nombre" class="control-label col-xs-4">Nombre</label>
        <div class="col-xs-8">
            <div class="input-group">
                <div class="input-group-addon">
                    <i class="fa fa-user"></i>
                </div>
                <input id="nombre" name="nombre" placeholder="Nombre" type="text" class="form-control" value="Steven">
            </div>
        </div>
    </div>
    <div class="form-group">
        <label for="rol" class="control-label col-xs-4">Tipo Rama Cientifica</label>
        <div class="col-xs-8">
            <select id="rol" name="rol" class="select form-control">
                <option value="natural">Natural</option>
                <option value="no_natural">No Natural</option>
            </select>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-xs-offset-4 col-xs-8">
            <button name="submit" type="submit" class="btn btn-success">Crear</button>
            <button name="reset" type="reset" class="btn btn-default">Limpiar</button>
        </div>
    </div>
</form>