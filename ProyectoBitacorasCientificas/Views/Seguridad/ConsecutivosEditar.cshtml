
@{
    ViewBag.Title = "ConsecutivosEditar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ConsecutivosEditar</h2>

<form class="form-horizontal">
    <div class="form-group">
        <label for="rol" class="control-label col-xs-4">Descripcion</label>
        <div class="col-xs-8">
            <select id="rol" name="rol" class="select form-control">
                <option value="proyectos">Proyectos</option>
                <option value="opcion2">Opcion2</option>
                <option value="opcion3">Opcion3</option>
                <option value="opcion4">Opcion4</option>
            </select>
        </div>
    </div>
    <div class="form-group">
        <label for="nombre" class="control-label col-xs-4">Consecutivo</label>
        <div class="col-xs-8">
            <div class="input-group">
                <div class="input-group-addon">
                    <i class="fa fa-user"></i>
                </div>
                <input id="consecutivo" name="consecutivo" placeholder="Consecutivo" type="text" class="form-control">
            </div>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-4">Prefijo</label>
        <div class="col-xs-8">
            <div class="custom-control custom-checkbox custom-control-inline">
                <input name="prefijo_0" id="prefijo_0" type="checkbox" class="custom-control-input" value="si" checked>
                <label for="prefijo_0" class="custom-control-label">Si</label>
            </div>
        </div>
    </div>
    <div class="form-group">
        <label for="nombre" class="control-label col-xs-4">Prefijo</label>
        <div class="col-xs-8">
            <div class="input-group">
                <div class="input-group-addon">
                    <i class="fa fa-user"></i>
                </div>
                <input id="prefijo_1" name="prefijo_1" placeholder="Prefijo" type="text" class="form-control" value="PRE_">
            </div>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-4">Rango</label>
        <div class="col-xs-8">
            <div class="custom-control custom-checkbox custom-control-inline">
                <input name="rango_0" id="rango_0" type="checkbox" class="custom-control-input" value="si" checked>
                <label for="rango_0" class="custom-control-label">Si</label>
            </div>
        </div>
    </div>
    <div class="form-group">
        <label for="nombre" class="control-label col-xs-4">Rango inicial</label>
        <div class="col-xs-8">
            <div class="input-group">
                <div class="input-group-addon">
                    <i class="fa fa-user"></i>
                </div>
                <input id="rango_inicial" name="rango_inicial" placeholder="Rango inicial" type="text" class="form-control" value="1">
            </div>
        </div>
    </div>
    <div class="form-group">
        <label for="nombre" class="control-label col-xs-4">Rango final</label>
        <div class="col-xs-8">
            <div class="input-group">
                <div class="input-group-addon">
                    <i class="fa fa-user"></i>
                </div>
                <input id="rango_final" name="rango_final" placeholder="Rango final" type="text" class="form-control" value="100">
            </div>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-xs-offset-4 col-xs-8">
            <button name="cambiar_password" class="btn btn-primary">Aceptar</button>
            <button name="submit" type="submit" class="btn btn-danger">Cancelar</button>
        </div>
    </div>
</form>