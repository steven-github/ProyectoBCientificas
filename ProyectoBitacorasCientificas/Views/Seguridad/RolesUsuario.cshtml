
@{
                /**/

                ViewBag.Title = "RolesUsuario";
                Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>RolesUsuario</h2>

<form class="form-horizontal">
    <div class="form-group">
        <label for="rol_laboratorio" class="control-label col-xs-4">Usuarios</label>
        <div class="col-xs-8">
            <select id="usuarios" name="usuarios" class="select form-control">
                <option value="cientifico">Josue</option>
                <option value="arquitecto">Steven</option>
                <option value="arquitecto">Daniela</option>
            </select>
        </div>
    </div>
    <div class="form-group">
        <label for="rol" class="control-label col-xs-4">Rol</label>
        <div class="col-xs-8">
            <select id="rol" name="rol" class="select form-control">
                <option value="administrador">Administrador</option>
                <option value="seguridad">Seguridad</option>
                <option value="consecutivo">Consecutivo</option>
                <option value="mantenimiento">Mantenimiento</option>
                <option value="consulta">Consulta</option>
            </select>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-xs-offset-4 col-xs-8">
            <button name="submit" type="submit" class="btn btn-primary">Guardar</button>
        </div>
    </div>
</form>
